{
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/rate-limit": "^10.1.1",
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.15",
    "nodemailer-express-handlebars": "^7.0.0",
    "pino": "^9.4.0",
    "prisma": "^5.19.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/fastify-jwt": "^0.8.1",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.16",
    "@types/supertest": "^6.0.2",
    "cpx": "^1.5.0",
    "dotenv-cli": "^7.4.2",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "build": "tsc && cpx \"src/templates/**/*.hbs\" dist/templates/",
    "dev": "dotenv -e .env ts-node src/server.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "generate": "dotenv -e .env -- npx prisma generate",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate dev",
    "migrate:prod": "dotenv -e .env -- npx prisma migrate deploy",
    "start": "dotenv -e .env node dist/server.js",
    "type-check": "tsc --noEmit",
    "seed": "dotenv -e .env -- ts-node prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
}
