{
  "dependencies": {
    "@fastify/formbody": "^8.0.0",
    "@fastify/jwt": "^9.0.0",
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^5.0.0",
    "pino": "^9.4.0",
    "prisma": "^5.19.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/fastify-jwt": "^0.8.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "dotenv-cli": "^7.4.2",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e .env.development ts-node src/server.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "generate": "dotenv -e .env.development -- npx prisma generate",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "start": "dotenv -e .env.production node dist/server.js",
    "type-check": "tsc --noEmit",
    "seed": "dotenv -e .env.development -- ts-node prisma/seed.ts"
  }
}
